{
  "name": "ShitBlocker",
  "version": "0.0.1",
  "short_name": "ShitBlocker",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "/dist/contentScript.js"
      ]
    }
  ]
}
